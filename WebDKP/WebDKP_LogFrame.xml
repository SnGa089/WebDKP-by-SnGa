     <!-- Input Box for character name to add or remove -->
      <EditBox name="$parentCharChange" inherits="WebDKPEditBoxTemplate" >
        <Size>
          <AbsDimension x="130" y="32"></AbsDimension>
        </Size>
        <Anchors>
          <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT">
