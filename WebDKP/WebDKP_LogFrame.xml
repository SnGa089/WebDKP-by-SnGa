              <Anchors>
                <Anchor point="RIGHT" relativePoint="RIGHT" >
                  <Offset>
                    <AbsDimension x="-137" y="0"/>
                  </Offset>
                </Anchor>
              </Anchors>
            </FontString>
          </Layer>
        </Layers>
        <Scripts>
          <OnLoad>
           getglobal(self:GetParent(self):GetName(self).."CharChange"):SetAutoFocus( false );
