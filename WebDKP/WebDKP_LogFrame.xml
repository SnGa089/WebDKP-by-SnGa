          <OnEscapePressed>
            getglobal(self:GetParent(self):GetName(self).."CharChange"):ClearFocus(self);
          </OnEscapePressed>
          <OnEnterPressed>
            <!-- Does nothing when you press enter currently -->
          </OnEnterPressed>
          <OnTextChanged>
           <!-- Does nothing when you change the text. It will only do something when you hit the button -->
          </OnTextChanged>
        </Scripts>
