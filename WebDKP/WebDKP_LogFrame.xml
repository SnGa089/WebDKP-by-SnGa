<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
..\..\FrameXML\UI.xsd">
  <!-- ================================================
       TEMPLATES 
       =================================================-->
  <!-- Defines a button template for all the buttons in the player list. This is 
  defined here so that these parameters don't need to be redefined for each button -->
  <Button name = "NameEntryTemplate" text="entry" virtual="true" >
    <Size>
      <AbsDimension x="150" y="16" />
    </Size>
  </Button>

  <!-- Defines a template for the log headers -->
  <Button name="WebDKP_LogTabTemplate" virtual="true">
    <Layers>
      <Layer level="BACKGROUND">
        <Texture name="$parentBackground">
          <Anchors>
            <Anchor point="TOPLEFT"/>
            <Anchor point="BOTTOMRIGHT"/>
          </Anchors>
          <Color r="1" g="1" b="1" a="1"/>
        </Texture>
      </Layer>
    </Layers>
    <Scripts>
      <OnLoad>
        getglobal(self:GetName(self) .. "Background"):SetVertexColor(.7, .7, .7, 0.5);
      </OnLoad>
      <OnEnter>
       getglobal(self:GetName(self) .. "Background"):SetVertexColor(0.5, 0.5, 0.5, 0.5); 
      </OnEnter>
      <OnLeave>
       getglobal(self:GetName(self) .. "Background"):SetVertexColor(.7, .7, .7, 0.5); 
      </OnLeave>
    </Scripts>
  </Button>
  

 

  <!-- Define a template for a log entry -->
  <Frame name="WebDKP_LogLineTemplate" virtual="true">
    <Size>
      <AbsDimension x="515" y="20"/>
    </Size>
    <Layers>
      <Layer level="BACKGROUND">
        <Texture name="$parentBackground">
          <Anchors>
            <Anchor point="TOPLEFT"/>
            <Anchor point="BOTTOMRIGHT"/>
          </Anchors>
          <Color r="1" g="1" b="1" a="1"/>
        </Texture>
      </Layer>
      <Layer level="ARTWORK">
        <FontString name="$parentAwarded" inherits="ChatFontNormal" text="">
          <Anchors>
            <Anchor point="LEFT">
              <Offset>
                <AbsDimension x="5" y="0"/>
              </Offset>
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parentAmount" inherits="ChatFontNormal" text="">
          <Anchors>
            <Anchor point="LEFT">
              <Offset>
                <AbsDimension x="115" y="0"/>
              </Offset>
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parentReason" inherits="ChatFontNormal" text="">
          <Anchors>
            <Anchor point="LEFT">
              <Offset>
                <AbsDimension x="180" y="0"/>
              </Offset>
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parentDate" inherits="ChatFontNormal" text="">
          <Anchors>
            <Anchor point="LEFT">
              <Offset>
                <AbsDimension x="400" y="0"/>
              </Offset>
            </Anchor>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
    <Scripts>
      <OnLoad>
        getglobal(self:GetName(self) .. "Background"):SetVertexColor(0, 0, 0, 0);
      </OnLoad>
      <OnEnter>
        WebDKP_HandleMouseOverLog(self);
      </OnEnter>
      <OnLeave>
        WebDKP_HandleMouseLeaveLog(self);
      </OnLeave>
      <OnMouseDown>
        WebDKP_SelectLogToggle(self);
      </OnMouseDown>
    </Scripts>
  </Frame>
  
