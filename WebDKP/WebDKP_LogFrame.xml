  <!-- ================================================
       MAIN FRAME
       =================================================-->
  <Frame name="WebDKP_LogFrame" toplevel="true" parent="UIParent" movable="true" hidden="true">
    <!-- EVENT HANDLERS FOR THE MASTER FRAME -->
    <Scripts>
      <OnMouseDown>
        self:StartMoving(self);
      </OnMouseDown>
      <OnMouseUp>
        self:StopMovingOrSizing(self);
      </OnMouseUp>
      <OnLoad>
        WebDKP_OnLoad(self);
      </OnLoad>
      <OnEvent>
        WebDKP_OnEvent(self);
      </OnEvent>
    </Scripts>
    <Size>
      <AbsDimension x="760" y="620"/>
    </Size>
    <Anchors>
      <Anchor point="LEFT"/>
    </Anchors>
    <Backdrop bgFile="Interface\TutorialFrame\TutorialFrameBackground" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
      <EdgeSize>
        <AbsValue val="16"/>
      </EdgeSize>
      <TileSize>
        <AbsValue val="16"/>
      </TileSize>
      <BackgroundInsets>
        <AbsInset left="5" right="5" top="5" bottom="5"/>
      </BackgroundInsets>
    </Backdrop>

